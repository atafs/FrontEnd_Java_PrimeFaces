package pt.americolib.bean;

import javax.faces.bean.ManagedBean;
import javax.faces.bean.SessionScoped;

@ManagedBean
@SessionScoped
public class KeyboardBean {
	
	//ATTRIBUTES
	private String value;

	//GETTERS AND SETTERS
	public String getValue() {
		return value;
	}

	public void setValue(String value) {
		this.value = value;
	}
}
